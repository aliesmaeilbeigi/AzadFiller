{
  "manifest_version": 3,
  "name": "Azad Filler",
  "version": "1.0",
  "description": "این افزونه به شما کمک می‌کنه تا جدول‌های ارزیابی اساتید دانشگاه آزاد رو سریع با گزینه متوسط پر و ثبت  کنید.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": ["https://stdn.iau.ir/*"],
  "web_accessible_resources": [
    {
      "resources": ["fonts/*"],
      "matches": ["<all_urls>"]
    }
  ]
}